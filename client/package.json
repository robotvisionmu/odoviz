    "npm-run-all": "^4.1.5",
    "npyjs": "^0.1.3",
    "numjs": "nicolaspanel/numjs",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-leaflet": "^2.4.0",
    "react-octicon": "^3.0.1",
